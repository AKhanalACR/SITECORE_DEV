<control xmlns:def="Definition" xmlns="http://schemas.sitecore.net/Visual-Studio-Intellisense">
  <xmlExpressUrlConflict>
    <FormDialog OKButton="OK" CancelButton="true">

      <!--<CodeBeside Type="Velir.SitecorePlugins.ExpressUrl.ExpressUrlConflictCodeBeside,Velir.SitecorePlugins"/>-->
      <CodeBeside Type="ACR.Foundation.ExpressUrls.ViewExpressURLCodeBeside,ACR.Foundation.ExpressUrls"/>
      
      <Border Class="">
        <Literal ID="Alert" Text="The Express URL you input conflicts with an Express Url assigned to the following item:" />
      </Border>

      <ul>
        <li class="">
          <Literal ID="ConflictedItem" />
        </li>
      </ul>

      <Border Class="">
        <Literal ID="AlertQuestion" Text="Would you like this Express URL deleted from the conflicting item?" />
      </Border>

    </FormDialog>
  </xmlExpressUrlConflict>
</control>
