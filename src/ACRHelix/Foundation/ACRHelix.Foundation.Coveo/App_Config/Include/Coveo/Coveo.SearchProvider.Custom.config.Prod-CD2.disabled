<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <sitecore>
    <coveo>
      <defaultIndexConfiguration>
        <securityConfiguration xdt:Transform="Replace">
          <indexPermissions>false</indexPermissions>
        </securityConfiguration>
        <farmName xdt:Transform="Replace">ACRHelix</farmName>
        <sitecoreUsername xdt:Transform="Replace">sitecore\acrcoveo</sitecoreUsername>
        <sitecorePassword xdt:Transform="Replace">yJqqGYM1mu6+cG9r4xgyXg==</sitecorePassword>
      </defaultIndexConfiguration>
    </coveo>

    <contentSearch>
      <configuration>
        <indexes>
          <index id="Coveo_master_index" xdt:Transform="Replace" xdt:Locator="Match(id)">
            <patch:delete />
          </index>
          <index id="Coveo_web_index" xdt:Locator="Match(id)">
            <strategies xdt:Transform="InsertIfMissing">
              <patch:delete />
            </strategies>
          </index>
        </indexes>
      </configuration>
    </contentSearch>
  </sitecore>
</configuration>