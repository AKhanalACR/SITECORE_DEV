/*accordion*/
.panel {
  border: 1px solid #d8d8d8;
  margin-bottom: 20px;
  padding: 5px 20px 5px 40px;
}

.panel a {
  color: #7b98ad;
}

.panel span {
  position: absolute;
  margin-left: -25px;
}

.panel-title a, .panel-title a:active {
  display: block;
  padding: 15px 0px;
  color: #7b98ad;
  text-decoration: none;
  font-weight: 700;
}

.panel-heading a:before {
  font-size: 30px;
  font-family: "Material Icons";
  content: "\e5c5";
  float: right;
  transition: all 0.3s;
  position: relative;
  bottom: 10px;
}

.panel-heading.active a:before {
  -webkit-transform: rotate(180deg);
  -moz-transform: rotate(180deg);
  transform: rotate(180deg);
}

.panel-body {
  padding-bottom: 15px;
  padding-right: 15px;
}

