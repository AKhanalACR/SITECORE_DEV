/*past-events*/
#events #events-flters {
  padding: 0;
  margin: 5px 0 20px 0;
  list-style: none;
  text-align: center;
}

#events #events-flters span {
  margin-right: 15px;
  text-transform: uppercase;
  font-weight: 700;
}

#events #events-flters li {
  cursor: pointer;
  margin: 15px 25px 15px 0;
  display: inline-block;
  margin-bottom: 5px;
  transition: all 0.3s ease-in-out;
  border-bottom: 3px solid #0f416b;
}

#events #events-flters li:hover,
#events #events-flters li.filter-active {
  border-bottom: 3px solid #e02a2e;
}

.event {
  text-align: center;
  margin-bottom: 20px;
  position: relative;
  overflow: hidden;
}

.event .event-info {
  cursor: pointer;
  opacity: 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  -ms-flex-pack: center;
  justify-content: center;
  -webkit-box-align: center;
