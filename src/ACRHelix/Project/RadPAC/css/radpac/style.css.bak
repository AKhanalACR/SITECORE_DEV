/* Back to top button */
.back-to-top {
  position: fixed;
  display: none;
  background: #e02a2e;
  color: #fff;
  width: 44px;
  height: 44px;
  text-align: center;
  line-height: 1;
  font-size: 16px;
  border-radius: 50%;
  right: 15px;
  bottom: 15px;
  transition: background 0.5s;
  z-index: 3;
}

.back-to-top i {
  padding-top: 12px;
  color: #fff;
}

