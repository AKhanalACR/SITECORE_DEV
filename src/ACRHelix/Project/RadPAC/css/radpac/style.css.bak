/*header*/
#header {
  transition: all 0.5s;
  z-index: 4;
  transition: all 0.5s;
  padding: 20px 0;
  background: #fff;
  box-shadow: 0px 0px 30px rgba(127, 137, 161, 0.3);
}

#header.header-scrolled,
#header.header-pages {
  padding: 10px 0;
}

#header .logo img {
  padding: 0;
  margin: 5px 0;
  max-height: 120px;
}

#header .logo a {
  position:relative;
  z-index:101;
}

/*banner*/
#banner {
  width: 100%;
  min-height: 308px;
  position: relative;
  background-size: cover;
  margin: 0 0 100px 0;
  border-bottom: 5px solid #ef6461;
}

.overlay-banner {
  background-color: #0f416b;
  position: absolute;
  top: 1px;
  left: 0px;
  padding: 0;
  height: 100%;
  width: 100%;
  opacity: .8;
}

.banner-wrapper {
  position: absolute;
  width: 100%;
  height: 100%;
}

#banner-backgroundImage {
  display: none;
}

#banner-background {
  height: 535px;
  top: 0px;
  left: 0px;
}

#banner .banner-video {
  width: 50%;
  float: left;
  top: 80px;
}

#banner iframe {
  border: 3px solid #6c989d;
  background: #666464;
}

#banner .banner-info {
  width: 50%;
  float: right;
  top: 90px;
}

#banner .banner-info p {
  text-align: center;
}

#banner .banner-video h1 {
  font-family: 'Slabo', serif;
  color: #fff;
  margin-bottom: 10px;
  font-size: 74px;
  font-weight: 500;
  text-align: center;
  position: relative;
  bottom: 25px;
}

.scroller {
  height: 200px;
  overflow: hidden;
  text-align: center;
}

.scroller-wrapper-gradient {
  width: 285px;
  margin: 10px auto;
  background: transparent;
  position: relative;
  box-sizing: border-box;
}

.scroller-gradient:after, .scroller-gradient::after {
  left: 0;
  z-index: 1;
  bottom: 0;
  content: '';
  position: absolute;
  width: 100%;
  height: 6em;
  background-image: linear-gradient(180deg, #1f517c, rgba(255, 255, 255, 0));
}

.scroller-gradient:after, .scroller-gradient::after {
  bottom: -34px;
  transform: rotate(180deg);
}

.scroller li {
  font-size: 20px;
  color: #fff;
  height: 40px;
  list-style: none;
  font-family: 'Slabo', serif;
}

.scroller ul {
  padding: 0px;
}

/*radpac button*/
.btn-radpac-red {
  position: relative;
  z-index: 5;
  background: #e02a2e;
  color: #fff;
  font-size: 20px;
  font-weight: 600;
  text-align: center;
  letter-spacing: 1px;
  padding: 10px 25px;
}

.btn-radpac-red:hover, .btn-radpac-red:focus {
  background: #FE0B10;
  border-color: #fff;
  color: #fff;
}

.btn-radpac-blue {
  background: #0f416b;
  color: #fff;
  font-size: 20px;
  font-weight: 600;
  text-align: center;
  letter-spacing: 1px;
  padding: 10px 20px;
}

.btn-radpac-blue:hover, .btn-radpac-blue:focus {
  background: #0c3355;
  border-color: #fff;
  color: #fff;
}

