<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore>
    <pipelines>
      <initializeContainer>
        <processor type= "ACRHelix.Feature.Navigation.Pipelines.InitializeContainer.RegisterDependencies, ACRHelix.Feature.Navigation" />
      </initializeContainer>
      <initialize>
        <processor type="ACRHelix.Feature.Navigation.Pipelines.SitemapRoute, ACRHelix.Feature.Navigation" patch:after="processor[@type='Sitecore.Mvc.Pipelines.Loader.InitializeRoutes, Sitecore.Mvc']" />
      </initialize>
    </pipelines>
  </sitecore>
</configuration>
