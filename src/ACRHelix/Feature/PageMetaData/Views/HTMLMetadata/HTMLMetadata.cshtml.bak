@inherits Glass.Mapper.Sc.Web.Mvc.GlassView<ACRHelix.Feature.HTMLMetadata.ViewModels.PageMetadataViewModel>
<!-- BEGIN NOINDEX -->
<title>@Model.PageTitle | American College of Radiology</title>
<!-- END NOINDEX -->
<meta name="description" content="@Model.MetaDescription">
<meta name="keywords" content="@Model.MetaKeywords">
<meta property="og:type" content="website" />
<meta property="og:title" content="@Model.OG_Title" />
<meta property="og:url" content="@Model.OG_URL" />
<meta property="og:description" content="@Model.OG_Description" />
@if (!string.IsNullOrWhiteSpace(Model.OG_Image))
{
<meta property="og:image" content="@Model.OG_Image" />
}