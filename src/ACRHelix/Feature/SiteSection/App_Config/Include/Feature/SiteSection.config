<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore>
    <commands>
     <command name="webeditcustom:removetile" type="ACRHelix.Feature.SiteSection.Commands.RemoveTileCommand, ACRHelix.Feature.SiteSection"/>
    </commands>
    <pipelines>
      <initializeContainer>
        <processor type= "ACRHelix.Feature.SiteSection.Pipelines.InitializeContainer.RegisterDependencies, ACRHelix.Feature.SiteSection" />
      </initializeContainer>
    </pipelines>
    <events>
      <event name="item:saved">
        <handler type="ACRHelix.Feature.SiteSection.Pipelines.ExternalTileSavedProcessor, ACRHelix.Feature.SiteSection" method="OnItemSaved"></handler>
      </event>
    </events>
  </sitecore>
</configuration>
